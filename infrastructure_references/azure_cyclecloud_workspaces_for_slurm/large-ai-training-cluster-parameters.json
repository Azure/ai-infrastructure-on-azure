{
    "location": {
      "value": "${LOCATION}"
    },
    "adminUsername": {
      "value": "${USERNAME}"
    },
    "adminPassword": {
      "value": "${PASSWORD}"
    },
    "adminSshPublicKey": {
      "value": "${SSH_PUBLIC_KEY}"
    },
    "ccVMName": {
      "value": "ccw-cyclecloud-vm"
    },
    "ccVMSize": {
      "value": "Standard_D4as_v5"
    },
    "resourceGroup": {
      "value": "${RESOURCE_GROUP_NAME}"
    },
    "sharedFilesystem": {
      "value": {
        "type": "anf-new",
        "anfServiceTier": "Premium",
        "anfCapacityInTiB": 4
      }
    },
    "additionalFilesystem": {
      "value": {
        "type": "aml-new",
        "lustreTier": "AMLFS-Durable-Premium-500",
        "lustreCapacityInTib": 512,
        "mountPath": "/data"
      }
    },
    "network": {
      "value": {
        "type": "new",
        "addressSpace": "${NETWORK_RANGE}",
        "bastion": false,
        "createNatGateway": true
      }
    },
    "databaseAdminPassword": {
      "value": "${DB_PASSWORD}"
    },
    "databaseConfig": {
      "value": {
        "type": "privateEndpoint",
        "databaseUser": "${DB_USERNAME}",
        "dbInfo": {
          "name": "${DB_NAME}",
          "id": "${MYSQL_ID}",
          "location": "${LOCATION}",
          "subscriptionName": ""
        }
      }
    },
    "acceptMarketplaceTerms": {
      "value": true
    },
    "slurmSettings": {
      "value": {
        "version": "24.05.4-2",
        "healthCheckEnabled": true
      }
    },
    "schedulerNode": {
      "value": {
        "sku": "Standard_D4as_v4",
        "osImage": "microsoft-dsvm:ubuntu-hpc:2204:latest"
      }
    },
    "loginNodes": {
      "value": {
        "sku": "Standard_F4s_v2",
        "osImage": "microsoft-dsvm:ubuntu-hpc:2204:latest",
        "initialNodes": 1,
        "maxNodes": 1
      }
    },
    "htc": {
      "value": {
        "sku": "Standard_D64ds_v6",
        "maxNodes": 100,
        "osImage": "microsoft-dsvm:ubuntu-hpc:2204:latest",
        "useSpot": false
      }
    },
    "hpc": {
      "value": {
        "sku": "Standard_D64ds_v6",
        "maxNodes": 1,
        "osImage": "microsoft-dsvm:ubuntu-hpc:2204:latest"
      }
    },
    "gpu": {
      "value": {
        "sku": "Standard_ND96isr_H100_v5",
        "maxNodes": 64,
        "osImage": "microsoft-dsvm:ubuntu-hpc:2204:latest"
      }
    },
    "tags": {
      "value": {}
    }
  }
  